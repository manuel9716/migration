 # para servicios

docker stop serviciosdev
docker rm serviciosdev
docker image rm image_serviciosdev
docker build --no-cache -t image_serviciosdev .
docker run --restart unless-stopped -d -p 4200:4200 --name serviciosdev image_serviciosdev
docker run -p 4200:4200 --name serviciosdev image_serviciosdev
image_serviciosdev
